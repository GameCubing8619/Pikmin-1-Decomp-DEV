DEPENDS += $(KANDO_FILES:.o=.d)

$(BUILD_DIR)/src/plugPikiKando/plugPikiKando.a: $(KANDO_FILES)
	@echo Linking... $@
	$(QUIET) mkdir -p $(dir $@)
	@echo $(KANDO_FILES) > build/KANDO_ofiles
	$(QUIET) $(LD) -library $(LIBRARY_LDFLAGS) -o $@ -lcf ldscript.lcf @build/KANDO_ofiles
